{
  "name": "vite-react-ts-antd-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-react-ts-antd-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-react-ts-antd-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npx eslint --fix"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,html,md}": [
      "npx prettier --write --loglevel warn"
    ],
    "src/**/*.{jsx,tsx,less,postcss,css,scss}": [
      "npx stylelint --fix --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@reduxjs/toolkit": "^1.9.1",
    "antd": "^4.24.4",
    "history": "^5.3.0",
    "moment": "^2.29.4",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.4",
    "react-router-dom": "^6.4.4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "rollup": "^2.79.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.18.0"
  }
}
